{
  "expo": {
    "name": "VisionBin",
    "slug": "visionbin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "visionbin-app",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/vision.png",
      "resizeMode": "contain",
      "backgroundColor": "#4B9858"
    },
   
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/vision.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/vision.png"
    },
    "extra": {
    },
    "owner": "pitw",
    "plugins": [
      "expo-router",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}